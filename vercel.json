{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.5.0"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.js" }
  ],
  "redirects": [
    {
      "source": "/form-submitted",
      "destination": "/thanks",
      "statusCode": 302
    }
  ],
  "crons": [
    {
      "path": "/api/weekly-report",
      "schedule": "0 10 * * 1"
    }
  ],
  "regions": ["syd1"]
}
