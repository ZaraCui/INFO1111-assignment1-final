{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/form-submitted",
      "status": 302,
      "headers": {
        "Location": "/thanks"
      }
    }
  ],
  "crons": [
    {
      "path": "/api/weekly-report",
      "schedule": "0 10 * * 1"
    }
  ],
  "regions": ["syd1"]
}
